				float Metallic = tex2DNode52.r;
				float Smoothness = pow( saturate( lerpResult60 ) , _SmoothnessPower );
				float Occlusion = ( 1.0 - ( _AOStrength * ( 1.0 - tex2DNode52.g ) ) );
				float Alpha = tex2DNode32.a;
				float AlphaClipThreshold = 0.5;
				float AlphaClipThresholdShadow = 0.5;
				float3 BakedGI = 0;
				float3 RefractionColor = 1;
				float RefractionIndex = 1;
				float3 Transmission = ( _TongueThickness * temp_output_100_0 ).rgb;
				float3 Translucency = ( _TongueSSS * 0.5 * temp_output_100_0 ).rgb;

				#ifdef ASE_DEPTH_WRITE_ON
					float DepthValue = 0;
				#endif

				#ifdef _CLEARCOAT
