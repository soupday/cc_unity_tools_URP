				float4 tex2DNode145 = tex2D( _BaseMap, uv_BaseMap );
				

				float Alpha = ( _BaseColor.a * tex2DNode145.a );
				float AlphaClipThreshold = _AlphaClip;
				float AlphaClipThresholdShadow = _ShadowClip;

